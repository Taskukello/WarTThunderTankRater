<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
		<% if current_user == user %>
        <td><%= link_to 'Show', user %></td>
		<% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
