<h1>Sign in</h1>

<%= form_tag session_path do %>
<div class="form-inline">
  <div class="form-group">
   <p> Username <%= text_field_tag :username, params[:username], class:"form-control" %> </p>
  </div>
  <div class="form-group">
  <p> Password <%= password_field_tag :password, params[:password], class:"form-control" %> </p>
  </div>
    </div>
  <%= submit_tag "Log in" %>
  </div>
<% end %>